{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "ignore": [
    "node_modules",
    "__pycache__",
    "*.pyc",
    ".env",
    "tests",
    "docs"
  ]
}
